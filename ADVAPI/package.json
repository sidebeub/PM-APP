{
  "name": "autodesk-viewer-app",
  "version": "1.0.0",
  "description": "An app for viewing 3D models using Autodesk Platform Services",
  "main": "src/server.js",
  "bin": "src/server.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "mkdir -p dist && cp -r src public uploads .env package.json package-lock.json dist/",
    "deploy": "npm run build && cd dist && npm install --production",
    "package": "pkg src/server.js --config pkg.config.json",
    "package:win": "pkg src/server.js --config pkg.config.json --target node14-win-x64 --output dist/advapi.exe",
    "build:win": "node package-win.js"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "form-data": "^4.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "pkg": "^5.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/autodesk-viewer-app"
  },
  "keywords": [
    "autodesk",
    "forge",
    "aps",
    "viewer",
    "3d",
    "model"
  ],
  "author": "",
  "license": "MIT"
}
